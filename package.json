{
  "name": "webuild.sg",
  "version": "2.14.12",
  "homepage": "http://webuild.sg",
  "keywords": "developer, designer, maker, engineer, open source, open events, open community",
  "private": true,
  "scripts": {
    "start": "node app",
    "test": "grunt travis --verbose && mocha test --recursive && mocha test --recursive --require blanket --reporter html-cov > coverage.html",
    "postinstall": "bower install && grunt build",
    "routine": "grunt cleanup && npm outdated && bower list && npm-check-updates -u && npm i && grunt && npm test && npm outdated"
  },
  "dependencies": {
    "body-parser": "1.14.0",
    "bower": "1.5.3",
    "cli-color": "1.0.0",
    "compression": "1.5.2",
    "cors": "2.7.1",
    "errorhandler": "1.4.2",
    "express": "4.13.3",
    "grunt-cli": "0.1.13",
    "grunt-contrib-stylus": "0.22.0",
    "grunt-contrib-uglify": "0.9.2",
    "grunt-jsbeautifier": "0.2.10",
    "ical": "0.4.0",
    "ical-generator": "0.2.4",
    "jade": "1.11.0",
    "jsonfile": "2.2.2",
    "moment-timezone": "0.4.0",
    "promise": "7.0.4",
    "request": "2.64.0",
    "serve-favicon": "2.3.0",
    "sitemap": "1.3.0",
    "webuild-events": "1.2.0",
    "webuild-repos": "1.1.0"
  },
  "devDependencies": {
    "blanket": "1.1.7",
    "bower": "1.5.3",
    "chai": "3.3.0",
    "coveralls": "2.11.4",
    "grunt": "0.4.5",
    "grunt-bump": "0.6.0",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-csslint": "0.5.0",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-stylus": "0.22.0",
    "grunt-contrib-uglify": "0.9.2",
    "grunt-jsbeautifier": "0.2.10",
    "grunt-jscs": "2.1.0",
    "mocha": "2.3.3",
    "mocha-lcov-reporter": "1.0.0",
    "nconf": "0.8.0"
  },
  "engines": {
    "iojs": "2.2.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "archives/index.js",
        "countdown/index.js",
        "events/eventbriteEvents.js",
        "events/facebookEvents.js",
        "events/index.js",
        "events/meetupEvents.js",
        "events/utils.js",
        "repos/index.js"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
